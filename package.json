{
    "name": "emata_coopmis_ui",
    "version": "0.1.0",
    "releaseNumber": "1.0.11",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "build-test": "set VUE_APP_ENV=test && vue-cli-service build",
        "build-staging": "set VUE_APP_ENV=staging && vue-cli-service build",
        "build-demo": "set VUE_APP_ENV=demo && vue-cli-service build",
        "build-production": "set VUE_APP_ENV=production && vue-cli-service build",
        "lint": "vue-cli-service lint",
        "test:unit": "vue-cli-service test:unit",
        "deploy": "msdeploy --verb sync --allowUntrusted true --source contentPath=%npm_package_config_sourcePath% --dest contentPath=%npm_package_config_destPath%,ComputerName=%npm_package_config_ComputerName%,UserName=%npm_package_config_UserName%,Password=%npm_package_config_Password%,includeAcls=False,authType=Basic",
        "package": "msdeploy --verb sync --source contentPath=%npm_package_config_sourcePath% --dest package=%npm_package_config_packagePath% --declareParam name='%npm_package_config_paramName%',defaultValue=%npm_package_config_paramValue%,kind=%npm_package_config_paramKind%,scope=%npm_package_config_paramScope%"
    },
    "dependencies": {
