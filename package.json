        "vue-ctk-date-time-picker": "^2.2.0",
        "vue-js-toggle-button": "^1.3.2",
        "vue-json-pretty": "^1.6.0",
        "vue-multiselect": "^2.1.6",
        "vue-router": "^3.0.3",
